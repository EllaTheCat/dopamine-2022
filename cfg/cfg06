# -*- Shell-script -*-
#
# etc i3 configuration file
# etc isn't et cetera, etc is EllaThcat!
#
# Pass the result of pressing two keys to i3-config bash $1 $2
#

# Row 3

mode "z_q" {
    bindsym q $send q q
    bindsym w $send q w
    bindsym e $send q e
    bindsym r $send q r
    bindsym t $send q t
    bindsym y $send q y
    bindsym u $send q u
    bindsym i $send q i
    bindsym o $send q o
    bindsym p $send q p
    bindsym a $send q a
    bindsym s $send q s
    bindsym d $send q d
    bindsym f $send q f
    bindsym g $send q g
    bindsym h $send q h
    bindsym j $send q j
    bindsym k $send q k
    bindsym l $send q l
    bindsym z $send q z
    bindsym x $send q x
    bindsym c $send q c
    bindsym v $send q v
    bindsym b $send q b
    bindsym n $send q n
    bindsym m $send q m

    bindsym 0 $send q 0
    bindsym 1 $send q 1
    bindsym 2 $send q 2
    bindsym 3 $send q 3
    bindsym 4 $send q 4
    bindsym 5 $send q 5
    bindsym 6 $send q 6
    bindsym 7 $send q 7
    bindsym 8 $send q 8
    bindsym 9 $send q 9

    bindsym space mode default
}

