    bindsym d $send c d
    bindsym f $send c f
    bindsym g $send c g
    bindsym h $send c h
    bindsym j $send c j
    bindsym k $send c k
    bindsym l $send c l
    bindsym z $send c z
    bindsym x $send c x
    bindsym c $send c c
    bindsym v $send c v
    bindsym b $send c b
    bindsym n $send c n
    bindsym m $send c m

    bindsym 0 $send c 0
    bindsym 1 $send c 1
    bindsym 2 $send c 2
    bindsym 3 $send c 3
    bindsym 4 $send c 4
    bindsym 5 $send c 5
    bindsym 6 $send c 6
    bindsym 7 $send c 7
    bindsym 8 $send c 8
    bindsym 9 $send c 9

    bindsym space mode default
}

